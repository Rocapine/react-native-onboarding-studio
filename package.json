{
  "name": "@rocapine/react-native-onboarding-studio",
  "version": "0.2.15",
  "description": "React Native client for Rocapine Onboarding Studio - Fetch and display onboarding steps from your CMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && cp -r src/assets dist/",
    "prepare": "npm run build",
    "watch": "tsc --watch",
